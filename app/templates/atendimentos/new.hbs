<div class="container">
	<div class="row">
		<div class="col-xs-12 col-sm-10 col-sm-offset-1 col-md-8 col-md-offset-2">
			<div class="page-header">
		  	<h1 class="text-info">Novo Atendimento</h1>
		  </div>
		</div>
	</div>

	<div class="row">
		<div class="col-xs-12 col-sm-10 col-sm-offset-1 col-md-8 col-md-offset-2">
			{{atendimento-form
			    atendimento = model.atendimento
			    pacientes = model.pacientes
			    consultorios = model.consultorios
			    disabled = false
			    cancelRoute = 'atendimentos'
			    buttonText = "Adicionar Atendimento"
			    submitAction=(route-action "adicionarAtendimento")
			}}
		</div>
	</div>
</div>