<div class="container">
	<div class="row">
		<div class="col-xs-12 col-sm-10 col-sm-offset-1 col-md-8 col-md-offset-2">
			<div class="page-header">
		  	<h1 class="text-info">Novo Atendimento</h1>
		  </div>
			{{atendimento-form
			    atendimento = model.atendimento
			    pacientes = model.pacientes
			    consultorios = model.consultorios
			    disabled = false
			    cancelRoute = 'atendimentos'
			    buttonText = "Adicionar Atendimento"
			    submitAction=(route-action "adicionarAtendimento")
			}}
		</div>
	</div>
	<hr/>
	<div class="row">
		<div class="col-xs-12 col-sm-10 col-sm-offset-1 col-md-8 col-md-offset-2">
			<div class="btn-group btn-group-justified" role="group" aria-label="...">
				<div class="btn-group" role="group">
					<a href="" class="btn btn-default">Cancelar</a>
				</div>
				<div class="btn-group" role="group">
					<a href="" class="btn btn-primary">S/N</a>
				</div>
				<div class="btn-group" role="group">
					<a href="" class="btn btn-success">NF</a>
				</div>
			</div>
		</div>
	</div>
</div>